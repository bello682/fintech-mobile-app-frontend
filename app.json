{
	"expo": {
		"name": "fintech-mobile-app-frontend",
		"slug": "fintech-mobile-app-frontend",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"jsEngine": "hermes",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true,
			"enableHermes": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"enableHermes": true
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
